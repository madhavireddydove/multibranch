pipeline { 
  
   agent any

   stages {
   
     stage('checkout') { 
        steps { 
           echo "code checkingout"
        }
     }
       stage('build') { 
        steps { 
           echo "code building"
        }
     }

         stage("Deploy artifact") { 
         steps { 
           echo "deploying artifact..."'
         }
       }     
  
   	}

   }
